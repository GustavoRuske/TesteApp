<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ruske.testeapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Splashscreen"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SignInActivity"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/ThemeOverlay.MyNoTitleActivity"
            >
        </activity>
        <activity
            android:name="com.example.ruske.testeapp.SignInActivityWithDrive"
            android:label="@string/app_name"
            android:theme="@style/ThemeOverlay.MyNoTitleActivity">
        </activity>
        <activity
            android:name="com.example.ruske.testeapp.IdTokenActivity"
            android:label="@string/app_name"
            android:theme="@style/ThemeOverlay.MyNoTitleActivity">
        </activity>

        <activity
            android:name="com.example.ruske.testeapp.ServerAuthCodeActivity"
            android:label="@string/app_name"
            android:theme="@style/ThemeOverlay.MyNoTitleActivity">
        </activity>

        <activity android:name=".Cadastrar" />
        <activity android:name=".Editar" />

    </application>

</manifest>